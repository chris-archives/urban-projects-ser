{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "routes": [
      {
        "src": "/robots.txt",
        "dest": "/robots.txt"
      },
      {
        "src": "/sitemap.xml",
        "dest": "/sitemap.xml"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }